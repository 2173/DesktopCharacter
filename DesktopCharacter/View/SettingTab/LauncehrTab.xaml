<UserControl x:Class="DesktopCharacter.View.SettingTab.LauncehrTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DesktopCharacter.View.SettingTab"
             xmlns:properties="clr-namespace:DesktopCharacter.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="LauncherGrid" Margin="0">
        <DataGrid x:Name="dataGrid">
            <DataGrid.Columns>
                <DataGridTextColumn Header="{x:Static properties:Resources.LauncherTab_Index}"/>
                <DataGridTextColumn Header="{x:Static properties:Resources.LauncherTab_ShortcutPath}"/>
                <DataGridTextColumn Header="{x:Static properties:Resources.LauncherTab_Path}" Width="Auto"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</UserControl>
